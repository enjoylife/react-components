{
  "name": "react-input-toggle",
  "version": "0.3.0",
  "description": "A collection of aesthetically pleasing, accessible and customizable toggles build with React.",
  "keywords": [
    "react",
    "react-component",
    "toggle",
    "switch",
    "input",
    "checkbox",
    "react-input"
  ],
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "github.com/enjoylife/react-input-toggle.git"
  },
  "bugs": "github.com/enjoylife/react-input-toggle/issues",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "start": "webpack-dev-server",
    "dist": "NODE_ENV=production webpack --config webpack.prod.config.js; node-sass lib/styles/switch.scss  dist/react-input-toggle.css",
    "test": "jest",
    "test:dev": "jest --watch",
    "cov": "codecov < ./coverage/lcov/lcov.info",
    "dev": "webpack-dev-server --inline --progress --colors --config -./webpack.config.js --host 0.0.0.0 --content-base ./",
    "ghpages": "NODE_ENV=production TARGET=minify webpack --config webpack.gh.config.js; git subtree push --prefix ghpages origin gh-pages"
  },
  "authors": [
    "Matthew Clemens"
  ],
  "license": "MIT",
  "devDependencies": {
    "babel-core": "^6.13.2",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-function-bind": "^6.22.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-2": "^6.13.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.27.0",
    "enzyme": "^2.7.1",
    "file-loader": "^0.10.1",
    "jest": "^19.0.2",
    "node-sass": "^4.5.0",
    "postcss-loader": "^1.3.3",
    "react-addons-test-utils": "^15.4.2",
    "react-highlight": "^0.9.0",
    "react-hot-loader": "^3.0.0-beta.6",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.13.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.1.0-beta.0",
    "webpack-notifier": "^1.5.0"
  },
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2"
  }
}
