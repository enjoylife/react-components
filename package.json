{
  "name": "react-input-toggle",
  "version": "0.3.0",
  "description": "A collection of aesthetically pleasing, accessible and customizable toggles build with React.",
  "keywords": [
    "react",
    "react-component",
    "toggle",
    "switch",
    "input",
    "checkbox",
    "react-input"
  ],
  "main": "./lib/index.js",
  "repository": {
    "type": "git",
    "url": "github.com/enjoylife/react-input-toggle.git"
  },
  "bugs": "github.com/enjoylife/react-input-toggle/issues",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "dist": "NODE_ENV=production webpack --config webpack.prod.config.js; node-sass lib/styles/switch.scss  dist/react-input-toggle.css",
    "test": "karma start --browsers Firefox --single-run",
    "test:dev": "karma start",
    "cov": "codecov < ./coverage/lcov/lcov.info",
    "dev": "webpack-dev-server --inline --progress --colors --config -./webpack.config.js --host 0.0.0.0 --content-base ./",
    "ghpages": "NODE_ENV=production TARGET=minify webpack --config webpack.gh.config.js; git subtree push --prefix ghpages origin gh-pages"
  },
  "authors": [
    "Matthew Clemens"
  ],
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^6.1.0",
    "autoprefixer-loader": "^3.1.0",
    "babel-core": "^6.1.4",
    "babel-helper-builder-react-jsx": "^6.0.15",
    "babel-loader": "^6.0.1",
    "babel-plugin-syntax-class-properties": "^6.0.14",
    "babel-plugin-syntax-object-rest-spread": "^6.0.14",
    "babel-plugin-transform-class-constructor-call": "^6.0.20",
    "babel-plugin-transform-class-properties": "^6.0.14",
    "babel-plugin-transform-function-bind": "^6.1.4",
    "babel-plugin-transform-react-jsx": "^6.0.18",
    "babel-preset-es2015": "^6.1.2",
    "babel-preset-react": "^6.1.2",
    "babel-preset-stage-0": "^6.1.2",
    "chai": "^3.4.1",
    "chai-spies": "^0.7.1",
    "classnames": "^2.2.0",
    "codecov.io": "^0.1.6",
    "css-loader": "^0.23.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.4",
    "isparta": "^4.0.0",
    "isparta-loader": "github:enjoylife/isparta-loader",
    "karma": "^0.13.15",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^1.0.1",
    "karma-cli": "^1.0.0",
    "karma-coverage": "^1.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-mocha": "^1.0.1",
    "karma-sourcemap-loader": "^0.3.6",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.3",
    "node-sass": "^3.4.1",
    "raw-loader": "^0.5.1",
    "react": "0.14.8",
    "react-addons-pure-render-mixin": "0.14.8",
    "react-addons-test-utils": "0.14.8",
    "react-dom": "0.14.8",
    "sass-loader": "^3.1.1",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.6",
    "webpack": "^1.12.3",
    "webpack-dev-server": "^1.12.1",
    "webpack-error-notification": "^0.1.4"
  },
  "peerDependencies": {
    "react": "0.14"
  },
  "dependencies": {}
}
